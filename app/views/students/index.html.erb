<%= link_to "Student Index", students_path %>
<%= link_to "Create a Student", new_student_path %>

<h1>All Students</h1>

<% @students.each do |student| %>
<p><%= link_to "#{student.name}", student_path(student) %></p>
<%= link_to "Delete #{student.name}", student_path(student), method: "delete" %>
<% end %>
